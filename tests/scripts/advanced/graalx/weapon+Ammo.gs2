//#CLIENTSIDE
function onWeaponFired()
{
  temp.muditem = new TStaticVar();
  temp.muditem.loadvarsfromarray(makevar("clientr.muditem_" @ client.selectedweapon));
 
  if (temp.muditem.itemtype == "ARCHETYPE_AMMO" && temp.muditem.quantity >= 1)
  {
    triggerserver("gui", name, "addammo", client.selectedweapon);
  }
}